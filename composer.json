{
    "name": "ngohr/castor",
    "description": "PHP MVC Router",
    "require": {
        "php": "5.6 | ^7"
    },
{
    "autoload": {
        "classmap": ["src/", "/", "castor/", "Castor.php"]
    }
}
	"autoload": {
        "files": ["Castor.php"],
		"exclude-from-classmap": ["src/", "plugins/"]
    }
}